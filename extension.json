{
	"name": "MW_EXT_Progress",
	"version": "1.0.0",
	"author": [
		"[https://kitsune.solar/ Kitsune Solar]",
		"[https://metastore.pro/ METASTORE]",
		"..."
	],
	"url": "https://metastore.pro/",
	"descriptionmsg": "mw-ext-progress-desc",
	"license-name": "[https://www.gnu.org/licenses/gpl-3.0.txt GPL-3.0]",
	"type": "parserhook",
	"require": {
		"MediaWiki": ">= 1.31.0",
		"extensions": {
			"MW_EXT_Kernel": "*"
		}
	},
	"AutoloadClasses": {
		"MediaWiki\\Extension\\MetaStore\\MW_EXT_Progress": "MW_EXT_Progress.class.php"
	},
	"Hooks": {
		"ParserFirstCallInit": [
			"MediaWiki\\Extension\\MetaStore\\MW_EXT_Progress::onParserFirstCallInit"
		],
		"BeforePageDisplay": [
			"MediaWiki\\Extension\\MetaStore\\MW_EXT_Progress::onBeforePageDisplay"
		]
	},
	"ExtensionMessagesFiles": {
		"MW_EXT_ProgressMagic": "MW_EXT_Progress.i18n.magic.php"
	},
	"MessagesDirs": {
		"MW_EXT_Progress": [
			"i18n"
		]
	},
	"ResourceModules": {
		"ext.mw.progress.styles": {
			"styles": [
				"styles/theme.css"
			],
			"position": "top",
			"targets": [
				"desktop",
				"mobile"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteExtPath": "MW_EXT_Progress/modules"
	},
	"manifest_version": 2
}
